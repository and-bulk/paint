{
  "extends": ["alloy", "alloy/react", "alloy/typescript", "@feature-sliced"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@feature-sliced/eslint-plugin-messages"],
  "processor": "@feature-sliced/messages/fs",
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "env": {
    "jest": true
  }
}
